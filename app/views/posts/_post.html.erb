<div class="col-lg-3 col-md-4 col-sm-6" id="post-feed-item">
  <%= link_to user_post_path(:user_id => current_user[:nickname], :id => post.id), :class => 'btn'  do %>
    <img src="<%= post.image.url %>" class="img-fluid" id="post-image">
    <div class="text-left">
      <span class="text-dark " id="post-title"><%= post.title %></span>
    </div>
    <div class="text-left" id='star_rating_<%= post.id %>'></div>
  <% end %>
</div>

<script>
$('#star_rating_<%= post.id %>').raty('destroy');
$('#star_rating_<%= post.id %>').raty({
  size: 20,
  number: 5,
  starOff: '/assets/star-off-10.png',
  starOn: '/assets/star-on-10.png',
  readOnly: true,
  score: 3
});
</script>
